{
  "name": "bacbo-pattern-analyzer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "chmod +x node_modules/.bin/vite "build": "vite build""build": "vite build" vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --host",
    "server": "node server/index.cjs",
    "scraper": "node server/scraper.cjs",
    "start:all": "concurrently --kill-others --names \"API,SCRAPER,FRONTEND\" --colors \"blue,yellow,green\" \"npm run server\" \"npm run scraper\" \"npm run dev\"",
    "start:scraper-only": "npm run scraper",
    "start:with-scraper": "concurrently --kill-others \"npm run scraper\" \"npm run dev\"",
    "start:frontend-only": "npm run dev",
    "start:api-only": "npm run server"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.1.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "lucide-react": "^0.263.1",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.64.0",
    "puppeteer": "^21.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@eslint/json": "^0.12.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "globals": "^16.3.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^4.4.5"
  }
}
